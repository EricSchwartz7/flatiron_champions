<div class="center-align">
<h2>Edit <%= @user.nickname %></h2>
</div>
<ul class="container">
<%= form_for @user do |f| %>
  <div class="row">
  <div class="col s12">
  <i class="material-icons prefix">account_circle</i>
    <%= f.label :nickname %>
    <%= f.text_field :nickname, class: 'input-field' %><br>
    </div>
  </div>
  <%= f.submit "Update Profile", class: 'update-profile-button' %>
<% end %>
</div>
